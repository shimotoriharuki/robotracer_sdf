<sdf version="1.5">
	<model name="robotracer">
		<static>false</static>

<!-- ****************** Link ****************** -->
		<link name="chassis">
			<pose>0 0 0.0015 0 0 0</pose>
			<inertial>
				<mass>0.03</mass>
				<inertia>
					<ixx>0.00005</ixx>
					<ixy>0.0</ixy>
					<ixz>0.0</ixz>
					<iyy>0.00005</iyy>
					<iyz>0.0</iyz>
					<izz>0.00005</izz>
				</inertia>
			</inertial>

			<collision name="collision_base">
				<geometry>
					<box>
						<size>0.1 0.1 0.001</size>
					</box>
				</geometry>
			</collision>
			<visual name="visual_base">
				<geometry>
					<!--
					<mesh>
						<uri>model://pioneer3at/meshes/chassis.dae</uri>
					</mesh>
					-->
					<box>
						<size>0.1 0.1 0.001</size>
					</box>
				</geometry>
			</visual>

			<collision name="collision_sensor_bar">
				<pose>0.0975 0 0.0 0 0 0</pose>
				<geometry>
					<box>
						<size>0.095 0.01 0.001</size>
					</box>
				</geometry>
			</collision>
			<visual name="visual_sensor_bar">
				<pose>0.0975 0 0.0 0 0 0</pose>
				<geometry>
					<box>
						<size>0.095 0.01 0.001</size>
					</box>
				</geometry>
			</visual>
			
			<collision name="collision_sensor">
				<pose>0.150 0.0 0.0 0 0 0</pose>
				<geometry>
					<box>
						<size>0.010 0.100 0.001</size>
					</box>
				</geometry>
			</collision>
			<visual name="visual_sensor">
				<pose>0.150 0.0 0.0 0 0 0</pose>
				<geometry>
					<box>
						<size>0.010 0.100 0.001</size>
					</box>
				</geometry>
			</visual>

			<collision name="collision_support_left">
				<pose>0.154 0.045 -0.001 0 0 0</pose>
				<geometry>
					<box>
						<size>0.001 0.001 0.001</size>
					</box>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>0</mu>
							<mu2>0</mu2>
							<slip1>1.0</slip1>
							<slip2>1.0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual_support_left">
				<pose>0.154 0.045 -0.001 0 0 0</pose>
				<geometry>
					<box>
						<size>0.001 0.001 0.001</size>
					</box>
				</geometry>
			</visual>
			<collision name="collision_support_right">
				<pose>0.154 -0.045 -0.001 0 0 0</pose>
				<geometry>
					<box>
						<size>0.001 0.001 0.001</size>
					</box>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>0</mu>
							<mu2>0</mu2>
							<slip1>1.0</slip1>
							<slip2>1.0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual_support_right">
				<pose>0.154 -0.045 -0.001 0 0 0</pose>
				<geometry>
					<box>
						<size>0.001 0.001 0.001</size>
					</box>
				</geometry>
			</visual>
		</link>

		<link name="right_front_wheel">
			<pose>0.011 -0.055 0.009 1.5707 0 0</pose>

			<inertial>
				<mass>0.005</mass>
				<inertia>
					<ixx>0.000001</ixx>
					<iyy>0.000001</iyy>
					<izz>0.000001</izz>
					<ixy>0</ixy>
					<ixz>0</ixz>
					<iyz>0</iyz>
				</inertia>
			</inertial>

			<collision name="collision">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>1</mu>
							<mu2>1</mu2>
							<slip1>0.5</slip1>
							<slip2>0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
			</visual>
		</link>
		<link name="left_front_wheel">
			<pose>0.011 0.055 0.009 1.5707 0 0</pose>

			<inertial>
				<mass>0.005</mass>
				<inertia>
					<ixx>0.000001</ixx>
					<iyy>0.000001</iyy>
					<izz>0.000001</izz>
					<ixy>0</ixy>
					<ixz>0</ixz>
					<iyz>0</iyz>
				</inertia>
			</inertial>

			<collision name="collision">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>1</mu>
							<mu2>1</mu2>
							<slip1>0.5</slip1>
							<slip2>0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
			</visual>
		</link>
		<link name="right_rear_wheel">
			<pose>-0.011 -0.055 0.009 1.5707 0 0</pose>

			<inertial>
				<mass>0.005</mass>
				<inertia>
					<ixx>0.000001</ixx>
					<iyy>0.000001</iyy>
					<izz>0.000001</izz>
					<ixy>0</ixy>
					<ixz>0</ixz>
					<iyz>0</iyz>
				</inertia>
			</inertial>

			<collision name="collision">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>1</mu>
							<mu2>1</mu2>
							<slip1>0.5</slip1>
							<slip2>0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
			</visual>
		</link>
		<link name="left_rear_wheel">
			<pose>-0.011 0.055 0.009 1.5707 0 0</pose>

			<inertial>
				<mass>0.005</mass>
				<inertia>
					<ixx>0.000001</ixx>
					<iyy>0.000001</iyy>
					<izz>0.000001</izz>
					<ixy>0</ixy>
					<ixz>0</ixz>
					<iyz>0</iyz>
				</inertia>
			</inertial>

			<collision name="collision">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>1</mu>
							<mu2>1</mu2>
							<slip1>0.5</slip1>
							<slip2>0</slip2>
						</ode>
					</friction>
				</surface>
			</collision>
			<visual name="visual">
				<geometry>
					<cylinder>
						<radius>0.01</radius>
						<length>0.01</length>
					</cylinder>
				</geometry>
			</visual>
		</link>
		
<!-- ****************** Joint ****************** -->
		<joint type="revolute" name="right_front">
			<pose>0 0 0 0 0 0</pose>
			<child>right_front_wheel</child>
			<parent>chassis</parent>
			<axis>
				<xyz>0 1 0</xyz>
				<use_parent_model_frame>true</use_parent_model_frame>
			</axis>
		</joint>
		<joint type="revolute" name="right_rear">
			<pose>0 0 0 0 0 0</pose>
			<child>right_rear_wheel</child>
			<parent>chassis</parent>
			<axis>
				<xyz>0 1 0</xyz>
				<use_parent_model_frame>true</use_parent_model_frame>
			</axis>
		</joint>
		<joint type="revolute" name="left_front">
			<pose>0 0 0 0 0 0</pose>
			<child>left_front_wheel</child>
			<parent>chassis</parent>
			<axis>
				<xyz>0 1 0</xyz>
				<use_parent_model_frame>true</use_parent_model_frame>
			</axis>
		</joint>
		<joint type="revolute" name="left_rear">
			<pose>0 0 0 0 0 0</pose>
			<child>left_rear_wheel</child>
			<parent>chassis</parent>
			<axis>
				<xyz>0 1 0</xyz>
				<use_parent_model_frame>true</use_parent_model_frame>
			</axis>
		</joint>
		<plugin name="SkidSteerDrivePlugin" filename="libSkidSteerDrivePlugin.so">
			<right_front>right_front</right_front>
			<right_rear>right_rear</right_rear>
			<left_front>left_front</left_front>
			<left_rear>left_rear</left_rear>
			<MaxForce>0.1</MaxForce>
		</plugin>
	
<!-- ****************** Line sensors **********************-->
	  <include>
		  <uri>model://robotracer_sdf/line_sensor</uri>
		  <pose>0.2 0 0.13 0 0 0</pose>
	  </include>
	  <joint name="line_sensor_joint" type="revolute">
		  <child>line_sensor::link</child>
		  <parent>chassis</parent>
		  <axis>
			  <xyz>0 0 1</xyz>
			  <limit>
				  <upper>0</upper>
				  <lower>0</lower>
			  </limit>
		  </axis>
	  </joint>
  </model>
</sdf>
